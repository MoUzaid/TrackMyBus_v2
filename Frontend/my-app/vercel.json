{
  "routes": [
    {
      "src": "/manifest.json",
      "headers": {
        "Content-Type": "application/manifest+json",
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/Header.png",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      }
    },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}